//Create Window
var mainWin = Ti.UI.createWindow({
	title: "Main Window",
	backgroundImage: "map.png"
});

var titleView = Ti.UI.createView({
	borderRadius : '5%',
	center : '0%',
	height : '40%',
	width : '60%',
	backgroundColor : '#fff'
});

//Window Main Code
mainWin.add(titleView);
mainWin.open();


